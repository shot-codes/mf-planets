{
  "name": "milky-way",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "(trap 'kill 0' SIGINT; vite dev & ./pocketbase/pocketbase serve)",
    "build": "vite build",
    "preview": "vite preview",
